<div class="container" >
  <h2 id="tag+category">Choose a Category</h2>

  <% @tags.each do |tag| %>

  <div id="category_box" class="floating-box"><img id="tag_image" src="<%= tag.image_location %>" style="height:80px;">
    <a href="/tags/<%= tag.id %>">
      <p ><%= tag.tag_category %></p>
    </a>

  </div>




  <% end %>

</div>
